{
  "name": "fullstackjs-bff",
  "private": true,
  "packageManager": "pnpm@7.17.1",
  "exports": "./dist/src/index.js",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "barrels": "barrelsby --config ./barrels.json",
    "about:summary": "pnpm config list 2>&1 | tee about.summary.log",
    "about": "(npm run-script && npm --versions && pnpm config list --json) 2>&1 | tee about.log",
    "eslint": "eslint --quiet --fix 'src/**'",
    "latest": "pnpm up -r --latest",
    "lint": "pnpm eslint && pnpm pretty",
    "pretty": "prettier --ignore-unknown --write",
    "proto": "pnpm -r exec $*",
    "types:debug": "tsc --extendedDiagnostics",
    "types:files": "tsc --listFilesOnly",
    "types:watch": "tsc --build --watch",
    "types:apps:watch": "pnpm types:watch",
    "types": "etsc",
    "start": "./node_modules/.bin/nodemon --config nodemon.json"
  },
  "dependencies": {
    "@nirvai-web/data": "1.0.0",
    "cookie": "0.5.0",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-jwt": "7.7.7",
    "express-jwt-permissions": "1.3.7",
    "express-session": "1.17.3",
    "express-unless": "2.1.3",
    "express-validator": "6.14.2",
    "fs-extra": "11.1.0",
    "got": "12.5.3",
    "helmet": "6.0.1",
    "jsonwebtoken": "8.5.1",
    "method-override": "3.0.0",
    "multer": "1.4.5-lts.1",
    "openapi-types": "12.1.0",
    "pg": "8.8.0",
    "pg-iterator": "0.2.7",
    "pg-mem": "2.6.4",
    "pg-monitor": "1.5.0",
    "pg-native": "3.0.1",
    "pg-promise": "10.15.4",
    "pino": "8.7.0",
    "pino-http": "8.2.1",
    "react": "18.2.0",
    "swagger-jsdoc": "6.2.5",
    "swagger-ui-express": "4.6.0",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@nirvai-web/types": "1.0.0",
    "@types/cookie": "0.5.1",
    "@types/express": "4.17.15",
    "@types/express-serve-static-core": "4.17.31",
    "@types/express-session": "1.17.5",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "29.2.4",
    "@types/jsonwebtoken": "8.5.9",
    "@types/multer": "1.4.7",
    "@types/node": "18.11.15",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@types/validator": "13.7.10",
    "barrelsby": "2.5.1",
    "cpy": "9.0.1",
    "esbuild": "0.16.7",
    "esbuild-node-tsc": "2.0.4",
    "esbuild-register": "3.4.1",
    "nodemon": "2.0.20",
    "pino-pretty": "9.1.1",
    "prettier": "2.8.1",
    "rimraf": "3.0.2",
    "tsc-watch": "6.0.0",
    "tslib": "2.4.1",
    "typescript": "4.9.4"
  }
}
