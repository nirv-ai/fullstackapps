{
  "name": "nirvai-fullstackjs",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "start": "echo nirvai booting....",
    "about:summary": "pnpm config list",
    "about": "npm run-script && npm --versions && pnpm config list",
    "eslint": "echo 'running eslint",
    "latest": "pnpm up -r --latest",
    "lint": "echo 'running eslint and prettier",
    "pretty": "echo 'running prettier",
    "proto": "pnpm -r exec $*",
    "postinstall": "manypkg check",
    "pkgfix": "manypkg fix"
  },
  "dependencies": {
    "@babel/core": "7.20.5",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@manypkg/cli": "0.19.2",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "husky": "8.0.2",
    "turbo": "1.6.3",
    "typescript": "4.9.4"
  },
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "bun-types": "^0.5.0"
  }
}
