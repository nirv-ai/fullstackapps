    "react-router-dom": "6.4.5",
    "react-scripts": "5.0.1",
    "validator": "13.7.0",
    "web-vitals": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@craco/craco": "7.0.0",
    "@craco/types": "7.0.0",
    "@nirvai-web/types": "1.0.0",
    "@types/node": "18.11.15",
