{
  "name": "fullstackjs-react",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@7.17.1",
  "homepage": "https://nirv.ai",
  "scripts": {
    "about:summary": "pnpm config list",
    "about": "npm run-script && npm --versions && pnpm config list --json",
    "eslint": "eslint --quiet --fix 'src/**'",
    "latest": "pnpm up -r --latest",
    "lint": "pnpm eslint && pnpm pretty",
    "pretty": "prettier --ignore-unknown --write",
    "proto": "pnpm -r exec $*",
    "types:debug": "tsc --extendedDiagnostics",
    "types:files": "tsc --listFilesOnly",
    "types:watch": "tsc --build --watch",
    "types:apps:watch": "pnpm types:watch",
    "types": "tsc --build",
    "start": "BROWSER=none WATCHPACK_POLLING=true FAST_REFRESH=false craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/baloo-2": "4.5.11",
    "@fontsource/inconsolata": "4.5.9",
    "@fontsource/indie-flower": "4.5.10",
    "@fontsource/kelly-slab": "4.5.9",
    "@fontsource/permanent-marker": "4.5.7",
    "@fontsource/rubik": "4.5.13",
    "@fontsource/shadows-into-light-two": "4.5.9",
    "@mui/icons-material": "5.11.0",
    "@mui/lab": "5.0.0-alpha.112",
    "@mui/material": "5.11.0",
    "@mui/system": "5.11.0",
    "@nirvai-web/data": "1.0.0",
    "@nirvai-web/dialogs": "1.0.0",
    "@nirvai-web/elements": "1.0.0",
    "@nirvai-web/forms": "1.0.0",
    "@nirvai-web/layout": "1.0.0",
    "@tanstack/query-core": "4.20.4",
    "@tanstack/query-sync-storage-persister": "4.20.4",
    "@tanstack/react-query": "4.20.4",
    "@tanstack/react-query-devtools": "4.20.4",
    "@tanstack/react-query-persist-client": "4.20.4",
    "@tanstack/react-table": "8.7.3",
    "axios": "1.2.1",
    "react": "18.2.0",
